@model mvcWebApp.Models.Gallery

<!DOCTYPE html>
<html>
<head>
    <title>Pet Portraiture</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Css -->
    <link href="assets/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" media="screen" />
    <link href="assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" media="screen" />
    <link href="assets/site/bigfont.css?ticks=@DateTime.Now.Ticks" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <article>

        @Html.Partial("_Header")

        @Html.Partial("_HeroUnit")

        @Html.Partial("_Gallery", Model)

        @Html.Partial("_Footer")

    </article>

    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="assets/bootstrap/dist/js/bootstrap.min.js"></script>
